<beans xmlns="http://www.springframework.org/schema/beans"
	 xmlns:p="http://www.springframework.org/schema/p" 
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://www.springframework.org/schema/beans 
	 http://www.springframework.org/schema/beans/spring-beans.xsd">
		<bean id="aaa" class="com.jbetoreyes.maven.springcore.Battery" p:name="AAA" p:price="2.5" p:rechargeable="true"/>
		<bean id="cdrw" class="com.jbetoreyes.maven.springcore.Disc" p:name="CD-RW" p:price="1.5" p:capacity="700"/>
		<bean id="dvdrw" class="com.jbetoreyes.maven.springcore.Disc" p:name="DVD-RW" p:price="3.0" p:capacity="700"/>
		<bean id="shoppingCart" class="com.jbetoreyes.maven.springcore.ShoppingCart" scope="prototype"/>
</beans>
